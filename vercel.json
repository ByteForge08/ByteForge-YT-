{
  "functions": {
    "api/download.py": {
      "runtime": "python@3.9"
    },
    "api/info.py": {
      "runtime": "python@3.9"
    },
    "api/health.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/public/$1"
    },
    {
      "src": "/download/video",
      "dest": "/api/download.py?type=video"
    },
    {
      "src": "/download/audio",
      "dest": "/api/download.py?type=audio"
    },
    {
      "src": "/api/info",
      "dest": "/api/info.py"
    },
    {
      "src": "/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
