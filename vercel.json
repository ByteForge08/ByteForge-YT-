{
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/download/video",
      "dest": "/api/download.py?type=video"
    },
    {
      "src": "/download/audio",
      "dest": "/api/download.py?type=audio"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
